<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8.1"/>
    </startup>
	<appSettings>
		<add key="HostSmtp" value="smtp.gmail.com"/>
		<add key="Port" value="587"/>
		<add key="EnableSsl" value="true"/>
		<add key="SenderName" value="Imie nazwisko"/>
		<add key="SenderEmail" value="adres mailowy z ktorego ma sie wysylac"/>
		<add key="SenderEmailPassword" value="encrypt:haslo"/>
		<add key="ReceiverEmail" value="adres mailowy na ktory ma sie wysylac"/>
		<add key="SendHour" value="8"/>
		<add key="IntervalInMinutes" value="60"/>
		<add key="IsReportSend" value="true"/>
	</appSettings>
</configuration>
